<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.pedro.mensajerosms.MainActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/colorPrimaryDark"
        android:visibility="visible">

        <Button
            android:id="@+id/btnClaves"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/btnFrecuencia"
            android:layout_alignParentTop="true"
            android:layout_alignStart="@+id/btnFrecuencia"
            android:layout_marginTop="134dp"
            android:text="Claves" />

        <Button
            android:id="@+id/btnFrecuencia"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="Frecuencias" />
    </RelativeLayout>
</android.support.constraint.ConstraintLayout>
